/**index.wxss**/
page{
  height: 100%;
  font: 17px/1.5 "Microsoft Yahei",Tahoma, Helvetica, Arial, "SimSun", sans-serif;
}
.page-container{
  height: 100%;
}
.page-body{
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
}
.page-info{
  flex: 1;
  width: 100%;
  background-color: #fff;
}
.page-info-title{
  width: 80%;
  text-align: center;
  font-size:20px;
  color: blue;
  margin: 15px auto;
}
.page-info-time{
  position: absolute;
  top:39%;
  left: 6%;
  text-align: left;
  line-height: 2.7;
}
.index-image{
  width: 100%;
  height: 200px;
}
.page-bottom{
  height: 55px;
  background-color:#eee;
  display: flex;
}
.page-bottom navigator{
  display: inline-block;
  flex: 1;
  border-left: 1px solid #ddd;
  line-height:3;
  text-align:center;
}
.active{
  background-color: #1AAD19;
  color: #fff;
}