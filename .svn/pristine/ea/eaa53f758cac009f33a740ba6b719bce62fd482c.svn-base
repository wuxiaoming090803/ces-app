/**index.wxss**/
page{
  height: 100%;
  width: 100%;
  font: 17px/1.5 "Microsoft Yahei",Tahoma, Helvetica, Arial, "SimSun", sans-serif;
}
.page-container{
   display: flex;
   flex-direction: column;
   height: 100%;
   width: 100%;
}
.page-info{
  flex: 1;
  width: 100%;
  background-color: #fff;
}
.page-info-title{
  width: 80%;
  text-align: center;
  font-size: 23px;
  margin: 25px auto;
}
.page-outer-cicle{
    background-color: #ddd;
    position: absolute;
    left:45%;
    top: 32%;
    margin-left: -100px;
    margin-top: -100px;
    border-radius: 50%; 
    width: 230px;
    height: 230px;
    border: 5px solid #ddd;
    background-image: linear-gradient(top, #999, #eaeceb 70% );
    box-shadow: outset rgba(0,0,0,0.2) 0px 5px 6px ;
}
.page-outer-cicle::after{
    content: "";
    position: absolute;
    left:50%;
    top: 50%;
    margin-left: -100px;
    margin-top: -100px;
    border-radius: 50%; 
    width: 200px;
    height: 200px;
    background: linear-gradient(180deg,#d9eff7 13%,#b1d3ea 56%,#b9e0ef);
    box-shadow: rgba(0,0,0,.4) 0 3px 6px;
}
.page-inner-cicle{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 2;
  font-size: 35px;
  width: 180px;
  text-align: center;
}

.page-info-time{
  position:absolute;
  left:28%;
  width: 300px;
  bottom:22%;
  font-size: 18px;
}
.page-info-durTime{
  margin-bottom: 20px;
}
.page-bottom{
  height: 55px;
  background-color:#eee;
  display: flex;
}
.page-bottom navigator{
  display: inline-block;
  flex: 1;
  border-left: 1px solid #ddd;
  line-height:3;
  text-align:center;
}
.active{
  background-color: #1AAD19;
  color: #fff;
}