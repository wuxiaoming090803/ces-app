/**index.wxss**/
page{
  height: 100%;
}
.page-container{
  height: 100%;
}
.page-body{
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
}
.page-info{
  flex: 1;
  background-color: #fff;
}
.page-info-title{
  width: 80%;
  text-align: center;
  font-size: 20px;
  margin: 15px auto;
  height: 5%;
}
map{
  width: 100%;
  height: 100%;
}
.page-bottom{
  height: 55px;
  z-index: 2;
  background-color:#eee;
  display: flex;
}
.page-bottom .nav{
  display: inline-block;
  flex: 1;
  border-left: 1px solid #ddd;
  line-height:3;
  text-align:center;
}
.active{
  background-color: #1AAD19;
  color: #fff;
}
