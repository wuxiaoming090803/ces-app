<!--index.wxml-->
<view class="index-container">
  <view class="index-bg-container">
      <image class="index-bg" src="../../imag/ces-bg.png"></image>
  </view>
   <view class="index-top-container">
       <image class="index-logo" src="../../imag/ces-logo.png"></image>
   </view>
   <view class="index-title">
    2017户外公益徒步活动
   </view>
    <view class="index-form-container">
        <form bindsubmit="formSubmit">
          <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
              <image class="index-userName" src="../../imag/username.png"></image>
               <input class="weui-input" name="name" type="text" bindinput= "userNameInput" placeholder="请输入用户名" />
            </view>
          </view>
          <view class="weui-cells weui-cells_after-title second">
            <view class="weui-cell weui-cell_input">
               <image class="index-phone" src="../../imag/phone.png"></image>
              <input class="weui-input" name="mobile" bindinput='userPhoneInput' type="number" placeholder="请输入手机号" />
            </view>
          </view>
          <view class='weui-cell weui-cell_checkbox'>
            <checkbox-group class="checkboxgroup" bindchange="checkboxChange">  
              <label class="checkbox">
                <checkbox value="cb" bindtap='checkbox'/>我是嘉宾
              </label>
            </checkbox-group>
          </view>
          <view class="btn-area">
            <button type="primary" formType="submit" bindtap='navigateTo'>确认</button>
          </view>
    </form>
   </view>
</view>