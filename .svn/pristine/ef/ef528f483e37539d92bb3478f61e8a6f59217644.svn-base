/**index.wxss**/
page{
  height: 100%;
}
.page-container{
  height: 100%;
}
.page-body{
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
}
.index-bg{
  width:100%;
height:100%;

}
.page-info{
  flex: 1;
  background-color: #fff;
  height: 92vh;
}
.page-info-title{
  width: 80%;
  text-align: center;
  font-size: 20px;
  margin: 15px auto;
  height: 5%;
}
map{
  width: 100%;
  height: 100%;
}
.page-bottom{
  height: 100rpx;
  z-index: 2;
  background-color:#fff;
  display: flex;
  font-size:35.5rpx;
}
.page-bottom .nav{
  display: inline-block;
  flex: 1;
  border-left: 1px solid #ddd;
  line-height:3;
  text-align:center;
  color: #333;
}
.page-bottom .active{
  background-color: #13b8e1;
  color: #fff;
}
