<view class="page-container">
      <view class="page-info">
       <view >初始化步数:{{ initSteps }}</view>
        <view class="page-info-title">您已贡献的步数为:</view>
        <view class="page-outer-cicle">
          <view class="page-inner-cicle">
           <text> {{ mySteps }} </text> 
           <text> 步</text>
          </view>

        </view>
        <view class="page-info-time">
           <view class="page-info-durTime">活动持续时间：{{ durationTime }}</view>
           <view class="page-info-endTime">活动剩余时间：{{ remainTime }}</view>
        </view>
      </view>
      <view class="page-bottom">
        <navigator url="../eventInfo/eventInfo" redirect="false" class="nav">活动信息</navigator>
        <navigator url="../ggq/ggq" redirect="false" >地图显示</navigator>
        <navigator url="userInfo" redirect="false" class="active">我的统计</navigator>
      </view>
</view>